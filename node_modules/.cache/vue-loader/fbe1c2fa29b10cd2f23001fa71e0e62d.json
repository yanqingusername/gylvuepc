load()\n        } else {\n          this.lastTime = new Date().getTime()\n        }\n      } else {\n        this.lastTime = new Date().getTime() // 如果在30分钟内点击，则把这次点击的时间记录覆盖掉之前存的最后一次点击的时间\n      }\n    }\n  }\n}\n</script>\n<style>\nhtml,body{width: 100%;height: 100%;margin: 0;padding: 0;}\n</style>"]}]}